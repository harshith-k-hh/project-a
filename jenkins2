pipeline {
    agent 

    stages {
        stage('stage1 clone') {
            steps {
                echo 'git clone the repo'
            }
        }
        stage('stage2 Build') {
            steps {
                echo 'Build the project'
            }
        }    
        stage('stage3  Deploy') {
            steps {
                echo 'Deploy the project to the team 1'
            }
        }
        stage('stage4  Deploy') {
            steps {
                echo 'Deploy the project to the team 2'
            }
        }       
        stage('stage5  Deploy') {
            steps {
                echo 'Deploy the project to the team 3'
            }
        }       
    }
}
